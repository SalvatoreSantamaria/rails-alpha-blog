<h1>Create an article</h1>

<%= form_for @article do |variable_name| %>
  <p>
    <%= variable_name.label :title %><br/>
    <%= variable_name.text_field :title %>
  </p>
  <p>
    <%= variable_name.label :description %><br/>
    <%= variable_name.text_area :description %>
  </p>

<p>
  <%= variable_name.submit %>
</p>

<% end %>





